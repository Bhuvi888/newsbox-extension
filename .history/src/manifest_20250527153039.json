{
  "manifest_version": 3,
  "name": "NewsBox",
  "version": "1.0",
  "action": {
    "default_popup": "/"
  },
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
