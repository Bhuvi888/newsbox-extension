{
  "manifest_version": 3,
  "name": "NewsBox",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": ""
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "96": "icon-96.png"
  }
}
